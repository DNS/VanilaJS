<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
	<ShortName>Search Name</ShortName>
	<Description>Search Description</Description>
	<InputEncoding>UTF-8</InputEncoding>
	<Image width="16" height="16">favicon.ico</Image>
	<Url type="text/html" method="GET" template="http://localhost/search?q={searchTerms}" />
	<Url type="application/x-suggestions+json" method="GET" template="http://localhost/suggestion?q={searchTerms}" />
	<SearchForm>http://localhost/</SearchForm>
</SearchPlugin>